{
  "manifest_version": 2,
  "name": "Yandex Music Native Controls",
  "version": "0.1",
  "permissions": [
    "http://*/*"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'; connect-src http://localhost:3007",
  "content_scripts": [
    {
      "matches": ["*://*.music.yandex.by/*"],
      "js": ["socket.io.js", "index.js"]
    }
  ]
}
